{
    // uplink format (DEFAULT/STANDARD/STE)
    "uplink_format": "DEFAULT",

    // ABP, OTAA, abp, otaa
    "mode": "ABP",

    // final_period = period Â± period_offset
    // Transmit period, unit: s
    "period": 5,
    // Transmit period random offset, unit: s
    "period_offset": 1,

    // EU868
    "datarate_scheme": "EU868",

    "rxwin2":{
        "freq":472.3,
        "dr": 0
    },

    // adaptive datarate option, true: enable, false: disable
    "adr":false,
    "dr": 0,                // default datarate DR0~DR15
    "power": 20,            // dBm
    "threshold": -55,       // Free channel threshold

    /* Oder of the sections decides, index can't ignored, member by which index is ignored
        the index will be counted automatically from last index, if first member's index is ignored,
        it starts from zero  */
    "channels":[
        {"index": 0, "freq": 471.5, "dr_min": 0, "dr_max": 5},
        {"index": 1, "freq": 471.7, "dr_min": 0, "dr_max": 5},
        {"index": 2, "freq": 471.9, "dr_min": 0, "dr_max": 5},
        {"index": 3, "freq": 472.1, "dr_min": 0, "dr_max": 5},
        {"index": 4, "freq": 472.3, "dr_min": 0, "dr_max": 5},
        {"index": 5, "freq": 472.5, "dr_min": 0, "dr_max": 5},
        {"index": 6, "freq": 472.7, "dr_min": 0, "dr_max": 5},
        {"index": 7, "freq": 472.9, "dr_min": 0, "dr_max": 5}
    ]
}
